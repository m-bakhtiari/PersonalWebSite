@model ErrorViewModel
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login V4</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/Contact/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Contact/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Contact/css/main.css">
</head>
<body>

    <div class="limiter">
        <div class="container-login100" style="background-image: url('~/images/profile/@Model.NotFoundPageBackground');">
            <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
                <span class="login100-form-title p-b-49">@Model.Description</span>
                <div class="container-login100-form-btn">
                    <div class="wrap-login100-form-btn">
                        <div class="login100-form-bgbtn"></div>
                        <a href="/" target="_self" class="login100-form-btn">صفحه اصلی</a>
                    </div>
                </div>
                <div class="flex-c-m">
                    <a href="@Model.InstagramUrl" class="login100-social-item bg-danger">
                        <i class="fa fa-instagram"></i>
                    </a>
                    <a href="@Model.TelegramUrl" class="login100-social-item bg-primary">
                        <i class="fa fa-telegram"></i>
                    </a>
                    <a href="@Model.WhatsAppUrl" class="login100-social-item bg-success">
                        <i class="fa fa-whatsapp"></i>
                    </a>
                </div>
            </div>
        </div>
    </div>

</body>
</html>